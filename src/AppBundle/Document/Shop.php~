<?php
namespace AppBundle\Document;
use Doctrine\ODM\MongoDB\Mapping\Annotations as MongoDB;

/**
 * @MongoDB\Document
 */
class Shop
{
    /**
     * @MongoDB\Id
     */
    protected $id;

    /**
     * @MongoDB\Field(type="string")
     */
    public $email;

    /**
     * @MongoDB\Field(type="string")
     */
    public $picture;
    /**
     * @MongoDB\Field(type="string")
     */
    public $city;
    /**
     * @MongoDB\Field(type="hash")
     */
    public $location = array();

}
